<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<GridPane alignment="center" hgap="10" prefHeight="444.0" prefWidth="435.0" vgap="10" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="sample.Controller">
   <children>
       <VBox prefHeight="471.0" prefWidth="462.0">
          <children>
              <Button fx:id="seedButton" text="Seeds" />
              <Button fx:id="opInterButton" text="Op Interface" />
              <Button fx:id="opImplButton" text="Op Implementation" />
              <Button fx:id="runButton" text="Run" />
            <ScrollPane fx:id="seedScroll" prefHeight="200.0" prefWidth="200.0">
               <content>
                  <BarChart fx:id="seedChart">
                    <xAxis>
                      <CategoryAxis fx:id="seedX" side="BOTTOM" />
                    </xAxis>
                    <yAxis>
                      <NumberAxis fx:id="seedY" side="LEFT" />
                    </yAxis>
                  </BarChart>
               </content>
            </ScrollPane>
          </children>
       </VBox>
   </children>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>


</GridPane>
